
subsection Dimension
  set dimension = 2
end

subsection Timer
  set enable timer = false
end

subsection Output
  set write vtu results = false
end

subsection Mesh
  set mesh file = ../../cylinder_2d.msh
end

subsection Time integration
  set dt        = 0.1
  set t_initial = 0
  set t_end     = 0.5
  set scheme    = BDF2
end

subsection Nonlinear solver
  set tolerance            = 1e-10
  set divergence_tolerance = 1e+4
  set max_iterations       = 50
  set enable_line_search   = true
  set analytic_jacobian    = true
end

subsection Linear solver
  subsection main physics
    set verbosity      = quiet
    set method         = direct_mumps
    set reuse          = true
  end
end

subsection FiniteElements
  set Velocity degree            = 2
  set Pressure degree            = 1
  set Mesh position degree       = 1
  set Lagrange multiplier degree = 2
end

subsection Physical properties
  set number of fluids = 1
  subsection Fluid 0
    set density             = 1
    set kinematic viscosity = 0.005
  end

  set number of pseudosolids = 1
  subsection Pseudosolid 0
    subsection lame lambda
      set Function expression = 1
    end
    subsection lame mu
      set Function expression = 1
    end
  end
end

subsection FSI
  set verbosity       = verbose
  set enable coupling = true
  set spring constant = 1
  set cylinder radius = 0.5
  set cylinder length = 1 # Unused in 2D 
end

subsection Initial conditions
  subsection velocity
    set Function expression = 1; 0
  end
end

subsection Fluid boundary conditions
  set verbosity = verbose
  set number = 4
  subsection boundary 0
    set id   = 1
    set name = Outlet
    set type = outflow
  end
  subsection boundary 1
    set id   = 2
    set name = Inlet
    set type = input_function

    subsection u
      set Function expression = 1
    end
    subsection v
      set Function expression = 0
    end
  end
  subsection boundary 2
    set id   = 3
    set name = NoFlux
    set type = slip
  end
  subsection boundary 3
    set id   = 4
    set name = InnerBoundary
    set type = weak_no_slip
  end
end

subsection Pseudosolid boundary conditions
  set number = 4
  subsection boundary 0
    set id   = 1
    set name = Outlet
    set type = fixed
  end
  subsection boundary 1
    set id   = 2
    set name = Inlet
    set type = fixed
  end
  subsection boundary 2
    set id   = 3
    set name = NoFlux
    set type = fixed
  end
  subsection boundary 3
    set id   = 4
    set name = InnerBoundary
    set type = coupled_to_fluid
  end
end

subsection Postprocessing
  #
  # Compute and print the forces on the cylinder
  #
  subsection forces computation
    set verbosity          = verbose
    set enable             = true
    set boundary id        = 4
    set write results      = false
    set computation method = lagrange multiplier
    set precision          = 12
  end

  #
  # Compute and print the position of the center of the cylinder
  #
  subsection structure position
    set enable           = true
    set boundary id      = 4
    set output prefix    = center
    set output frequency = 1
    set precision        = 10
  end
end